   "source": [
    "# 生成斐波那契数列"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def fib(n):\n",
    "    \"\"\"\n",
    "    生成斐波那契数列 [1, 1, 2, 3, 5....]\n",
    "    参数:\n",
    "        n 数列的个数\n",
    "    返回值:\n",
    "        [1, 1, 2, 3, 5....](n个值)\n",
    "    \"\"\"\n",
    "    \n",
    "    l = [1, 1]\n",
    "    [l.append(l[-2]+l[-1]) for _ in range(n-len(l))]\n",
    "    \n",
    "    return l"
   ]
  },
